<!-- -->
<launch>

    <arg name="vehicle_name" default="mks"/>
    <arg name="phone1_name" default="android/galaxy1"/>
    <arg name="phone2_name" default=""/>
    <arg name="phone3_name" default=""/>
    <arg name="phone_names" default="$(arg phone1_name)"/>
    <arg name="lidar_topic_name" default="velodyne_points" />
    <arg name="rate" default="1.0"/>
    <arg name="start" default="50"/>

    <arg name="pf_gps_xy_bias" default="10.0" />

    <include file="/media/ohashi/a65390d1-3a01-41f6-bcb9-97c998a6b643/dataset/UCB/191116/good1.launch">
        <arg name="rate" value="$(arg rate)"/>
        <arg name="start" value="$(arg start)"/>
    </include>

    <include file="$(find smartphone_localizer)/launch/phone_localizer.launch">
        <arg name="device_name" value="$(arg phone1_name)" />
        <arg name="output" value="screen" />
        <arg name="pf_gps_xy_bias" value="$(arg pf_gps_xy_bias)"/>
    </include>

    <include file="$(find smartphone_localizer)/launch/vehicle_phones_manager.launch">
        <arg name="vehicle_name" default="$(arg vehicle_name)"/>
        <arg name="phone_names" value="$(arg phone_names)"/>
        <arg name="points_raw" value="$(arg lidar_topic_name)"/>
    </include>

      <node type="rqt_console" name="rqt_console" pkg="rqt_console" />
      <node type="rqt_logger_level" name="rqt_logger_level" pkg="rqt_logger_level" />
      <node type="rqt_graph" name="rqt_graph" pkg="rqt_graph" />
      <node type="rviz" name="rviz" pkg="rviz" args="-d $(find smartphone_localizer)/rviz/phone_localizers.rviz" />

</launch>
